"vertexlitgeneric"
{
	"$baseTexture" "Models/Weapons/v_crossbow/crossbow dirtmap"
	"$bumpmap" "Models/Weapons/v_crossbow/normal_map_composite copy"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.25 .25 .25]"
	"$normalmapalphaenvmapmask" 1

// -- From here down is new stuff which will only be applied if $phong is set to 1 --
	"$phong" "1"
	"$phongboost"	"3"
	"$phongexponent" "50"
	"$phongfresnelranges"	"[.1 3 1]"

	"360?$color2" "[ 0.7 0.7 0.7 ]"
	
	"$glowcolor" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 			"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 			"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 			"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}

	
}
"vertexlitgeneric"
{
	"$baseTexture" "Models/Weapons/V_Rocket_launcher/rocket launcher sheet"
	"$alphatest" "1"
	"360?$color2" "[ 0.8 0.8 0.8 ]"

	"$bumpmap" "Models/Weapons/V_Rocket_launcher/flat_normal"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost"	"2"
	"$phongfresnelranges"	"[ 0.2 0.5 1.5 ]"
	
	"$glowcolor" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 			"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 			"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 			"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}

	
}

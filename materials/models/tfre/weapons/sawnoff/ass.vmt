"VertexLitGeneric"
{
	"$basetexture" "models/tfre/weapons/sawnoff/Ass"
	"$bumpmap" "models/tfre/weapons/sawnoff/Ass_NM"
	"$surfaceprop" "metal"
	"$model" 1
	"$phong"				"1"
	"$phongboost"			"1.5"
	"$phongexponent"			"32"
	"$phongtint"			"[0.3 0.3 0.3]"
	"$phongfresnelranges"		"[0.4 0.4 0.4]"
	"$rimlight" 				"12"	
	"$rimlightexponent" 			"50"
	"$rimlightboost"			"0.1"

	"$glowcolor" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 			"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 			"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 			"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}


}

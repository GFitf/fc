"VertexLitGeneric"
{
	$basetexture "models/tfre/weapons/sawnoff/sawnoff_col"
	$bumpmap "models/tfre/weapons/sawnoff/sawnoff_nor"


	"$normalmapalphaenvmapmask" 1
	"$surfaceprop" "metal"

	"$envmapconstrast" "1"
	$envmapsaturation 0.1

	"$phong"     "1"
	"$phongexponent"  "4"

	"$phongboost"   4
	"$phongfresnelranges"   "[.75 .5 1]"
	"$phongalbedotint" "1"

	"$halflambert" "1"

	"$rimlight" "1"
	"$rimlightexponent" "512"	
	"$rimexponent" "512"
	"$rimlightboost" ".1"

	"$detail" "models/tfre/weapons/sawnoff/sawnoff_detail"	
	"$detailscale" "1"
	"$detailblendfactor" 0
	"$detailblendmode" 1
	
	"$envmap" "models/tfre/weapons/sawnoff/cubemap_gold001"
	"$envmaptint" "[0 0 0]"
	
	"$glowcolor" "1"
	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 			"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 			"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 			"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}

}